{
  "framework": "qunit",
  "test_page": "test/index.html",
  "cwd": "",
  "port": 8080,
  "launchers": {
    "SL_Chrome_Current": {
      "command": "saucie --browserNameSL='chrome' --versionSL='43.0' --platformSL='OS X 10.10' --ct=0",
      "protocol": "tap"
    },
    "SL_Chrome_Last": {
      "command": "saucie --browserNameSL='chrome' --versionSL='42.0' --platformSL='OS X 10.10' --ct=0",
      "protocol": "tap"
    },

    "SL_Firefox_Current": {
      "command": "saucie --browserNameSL='firefox' --versionSL='38.0' --platformSL='Linux' --ct=0",
      "protocol": "tap"
    },
    "SL_Firefox_Last": {
      "command": "saucie --browserNameSL='firefox' --versionSL='37.0' --platformSL='Linux' --ct=0",
      "protocol": "tap"
    },

    "SL_Safari_Current": {
      "command": "saucie --browserNameSL='safari' --versionSL='8.0' --platformSL='OS X 10.10' --ct=0",
      "protocol": "tap"
    },
    "SL_Safari_Last": {
      "command": "saucie --browserNameSL='safari' --versionSL='7.0' --platformSL='OS X 10.9' --ct=0",
      "protocol": "tap"
    },

    "SL_Opera_Current": {
      "command": "saucie --browserNameSL='opera' --versionSL='12.15' --platformSL='Linux' --ct=0",
      "protocol": "tap"
    },
    "SL_Opera_Last": {
      "command": "saucie --browserNameSL='opera' --versionSL='11.64' --platformSL='Windows 7' --ct=0",
      "protocol": "tap"
    },

    "SL_IE_11": {
      "command": "saucie --browserNameSL='internet explorer' --versionSL='11.0' --platformSL='Windows 7' --ct=0",
      "protocol": "tap"
    },
    "SL_IE_10": {
      "command": "saucie --browserNameSL='internet explorer' --versionSL='10.0' --platformSL='Windows 7' --ct=0",
      "protocol": "tap"
    },
    "SL_IE_9": {
      "command": "saucie --browserNameSL='internet explorer' --versionSL='9.0' --platformSL='Windows 7' --ct=0",
      "protocol": "tap"
    }
  },
  "watch_files": [
    "packages/**/*.js"
  ],
  "launch_in_dev": [
    "PhantomJS",
    "Chrome",
    "Firefox"
  ],
  "launch_in_ci": [
    "SL_Chrome_Current",
    "SL_Chrome_Last",
    "SL_Firefox_Current",
    "SL_Firefox_Last",
    "SL_Safari_Current",
    "SL_Safari_Last",
    "SL_Opera_Current",
    "SL_Opera_Last",
    "SL_IE_11",
    "SL_IE_10",
    "SL_IE_9"
  ]
}